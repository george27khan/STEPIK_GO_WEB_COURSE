# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type Item {
  id: ID!
  name: String!
  inStock: Int!
  sellerId: ID!
  catalogId: ID!
}

type Seller {
  id: ID!
  name: String!
  deals: String!
}

type Catalog {
	id:       ID!
	name:     String!
	childs: [Child!]!
}

type Child {
	id:       Int!
	name:     String!
}

type Query {
  # query{ Catalog(ID: "1") { id name childs { id name } } }
  Catalog(ID: ID!): Catalog!
}